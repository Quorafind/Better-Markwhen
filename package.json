{
	"name": "markwhen",
	"version": "developer",
	"description": "Markwhen integration for Obsidian.md",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"watch": "npm run dev & watch ./copyAssets.sh",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint ./src",
		"vite": "vite build --watch & watch ./copyAssets.sh"
	},
	"keywords": [],
	"author": "Rob Koch",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.12.7",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"obsidian": "^1.5.7-1",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vite": "^5.2.10",
		"watch": "^1.0.2"
	},
	"dependencies": {
		"@codemirror/state": "^6.4.1",
		"@codemirror/view": "^6.26.3",
		"@markwhen/calendar": "^1.3.4",
		"@markwhen/oneview": "^1.0.0",
		"@markwhen/parser": "^0.10.15",
		"@markwhen/timeline": "^1.3.3",
		"@markwhen/view-client": "^1.4.4"
	}
}
